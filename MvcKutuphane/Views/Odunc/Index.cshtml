@using MvcKutuphane.Models.Entity
@model List<TBL_HAREKET>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Layout.cshtml";
}






<table class="table table-bordered">


    <tr class="bg-dark" style="color:white">
        <td> İŞLEM ID</td>
        <td>KİTAP ADI</td>
        <td>ÜYE</td>
        @*<td>PERSONEL</td>*@
        <td>ALIŞ TARİHİ</td>
        <td>İADE TARİHİ</td>
        

        <td>iŞLEMLER</td>



    </tr>
    @foreach (var x in Model)
    {

<tr>
    <td>@x.ID</td>
    <td>@x.TBL_KITAP.AD</td>
    <td>@x.TBL_UYELER.AD&nbsp @x.TBL_UYELER.SOYAD</td>
    @*<td> @x.TBL_PERSONEL.ID</td>*@
    <td>@Convert.ToDateTime(x.ALISTARIH).ToString("dd/MM/yyyy")</td>
    <td> @Convert.ToDateTime(x.IADETARIH).ToString("dd/MM/yyyy")</td>
   

    <td>

        <a class="btn btn-success" href="/Odunc/Odunciade/@x.ID">Kitabı İade Et</a>
    </td>

</tr>

    }



</table>



